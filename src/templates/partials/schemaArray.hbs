{
	type: 'array',
{{#if link}}
	contains: {{>schema link}},
{{else}}
	contains: {
		type: '{{{getModuleType base}}}',
	},
{{/if}}
{{#if isReadOnly}}
	isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
	isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
	isNullable: {{{isNullable}}},
{{/if}}
}
